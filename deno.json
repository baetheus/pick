{
  "name": "@baetheus/pick",
  "version": "0.14.0",
  "unstable": [
    "bundle"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "exports": {
    ".": "./mod.ts",
    "./router": "./router.ts",
    "./builder": "./builder.ts",
    "./builder_client": "./builder_client.ts",
    "./builder_server": "./builder_server.ts",
    "./builder_static": "./builder_static.ts",
    "./tokens": "./tokens.ts"
  },
  "imports": {
    "@baetheus/fun": "jsr:@baetheus/fun@^3.0.0-rc.8",
    "@baetheus/pick": "jsr:@baetheus/pick@^0.11.0",
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/fs": "jsr:@std/fs@^1.0.17",
    "@std/media-types": "jsr:@std/media-types@^1.1.0",
    "@std/path": "jsr:@std/path@^1.0.8",
    "preact": "npm:preact@^10.24.0",
    "preact-iso": "npm:preact-iso@^2.6.0",
    "preact-render-to-string": "npm:preact-render-to-string@^6.5.0"
  },
  "include": [
    "LICENSE",
    "README.md",
    "deno.json"
  ],
  "exclude": [
    "./testing/fixtures/*"
  ],
  "tasks": {
    "test": "deno test --allow-read=testing/fixtures --allow-write"
  }
}
